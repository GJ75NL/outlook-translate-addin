<?xml version="1.0" encoding="UTF-8"?>
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="TaskPaneApp">
  <Id>2b7f66e0-3b43-4cfd-9230-df4c2d0b2fa9</Id>
  <Version>1.0.0.0</Version>
  <ProviderName>Jouw Naam</ProviderName>
  <DefaultLocale>en-US</DefaultLocale>
  <DisplayName DefaultValue="AI Translate" />
  <Description DefaultValue="Vertaal e-mails tussen Spaans, Engels en Nederlands." />
  <IconUrl DefaultValue="https://via.placeholder.com/32" />
  <SupportUrl DefaultValue="https://jouwdomein.nl/support" />
  <Hosts>
    <Host Name="Mailbox" />
  </Hosts>
  <Requirements>
    <Sets DefaultMinVersion="1.1">
      <Set Name="Mailbox" />
    </Sets>
  </Requirements>
  <FormSettings>
    <Form xsi:type="ItemRead">
      <DesktopSettings>
        <SourceLocation DefaultValue="https://gj75nl.github.io/outlook-translate-addin/index.html" />
        <RequestedHeight>250</RequestedHeight>
      </DesktopSettings>
    </Form>
    <Form xsi:type="ItemEdit">
      <DesktopSettings>
        <SourceLocation DefaultValue="https://gj75nl.github.io/outlook-translate-addin/index.html" />
        <RequestedHeight>250</RequestedHeight>
      </DesktopSettings>
    </Form>
  </FormSettings>
  <Permissions>ReadWriteItem</Permissions>
</OfficeApp>
